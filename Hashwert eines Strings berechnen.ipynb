   "source": [
    "import hashlib\n",
    "\n",
    "s1 = \"Hallo\"\n",
    "s2 = \" zusammen\"\n",
    "\n",
    "H = hashlib.sha224()\n",
    "H.update(s1)\n",
    "H.update(s2)\n",
    "h1 = H.hexdigest()\n",
    "\n",
    "H = hashlib.sha224()\n",
    "H.update('%s%s' % (s1, s2))\n",
    "h2 = H.hexdigest()\n",
    "\n",
    "H.update('%s%s' % (s1, s2))\n",
    "h3 = H.hexdigest()\n",
    "\n",
    "print(h1 == h2)\n",
    "print(h2 == h3)"
   ]
